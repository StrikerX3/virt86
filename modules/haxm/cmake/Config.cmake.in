@PACKAGE_INIT@

find_package(virt86-core CONFIG REQUIRED)
if(WIN32)
    find_package(virt86-sys-windows CONFIG QUIET)
elseif(LINUX)
    find_package(virt86-sys-linux CONFIG QUIET)
elseif(APPLE)
    find_package(virt86-sys-darwin CONFIG QUIET)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/@TARGETS_EXPORT_NAME@.cmake")
check_required_components("@PROJECT_NAME@")
